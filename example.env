# Copy this file to .env and adjust values for your environment.

# Agent runtime
AGENT_IMPL=zeroclaw
LLM_BACKEND=openrouter
MODEL_NAME=arcee-ai/trinity-large-preview:free
ZEROCLAW_AUTO_UPDATE=false

# OpenRouter credentials (required when LLM_BACKEND=openrouter)
OPENROUTER_API_KEY=

# Optional OpenRouter/OpenAI-compatible endpoint override
OPENAI_BASE_URL=https://openrouter.ai/api/v1

# Optional: protect web operator actions (task submission) with header token
# Web UI sends this as x-ops-token.
WWS_WEB_TOKEN=

# Local model options (used when LLM_BACKEND=ollama or local)
LOCAL_MODEL_PATH=./models/gpt-oss-20b.gguf

# Optional E2E toggles
E2E_LIVE_LLM=0
E2E_FAULT=0
E2E_SOAK=0
E2E_PLAYWRIGHT=0
E2E_PLAYWRIGHT_REAL_30=0
